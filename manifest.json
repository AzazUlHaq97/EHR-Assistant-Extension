{
    "manifest_version": 3,
    "name": "EHR Voice Command",
    "version": "1.0",
    "description": "Control MediTech with voice commands.",
    "permissions": [
        "storage",
      "tabs",
      "scripting"
    ],

    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "https://mtest.aku.edu/*",
      "https://*/*", 
      "http://*/*" 
    ]  
  }
  